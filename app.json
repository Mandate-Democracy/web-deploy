{
  "expo": {
    "name": "MD App Demo",
    "slug": "md-app-demo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "baseUrl": "/web-deploy"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6daf5c66-0793-4ce4-b90d-d671c08d5569"
      }
    }
  }
}
